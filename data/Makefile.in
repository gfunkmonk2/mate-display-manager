	if test '!' -d $(DESTDIR)$(mdmconfdir); then \
		$(mkinstalldirs) $(DESTDIR)$(mdmconfdir); \
		chmod 755 $(DESTDIR)$(mdmconfdir); \
	fi
	if test '!' -f $(DESTDIR)$(MDM_CUSTOM_CONF); then \
		$(INSTALL_DATA) mdm.conf-custom $(DESTDIR)$(MDM_CUSTOM_CONF); \
